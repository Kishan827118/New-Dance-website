extends base.pug

block script
    script(src='/static/index.js' )

block style 
    style
        include ../static/contactstyle.css
        include ../static/style.css
        //- include ../static/submit.pug 
        

block content 
    div#container
        h1 Contact Us 
        form(action="/contact", method = "post" class="Myform")
            input(type="text" class= "Myinput" name="name" placeholder ="Enter Name") 
            input(type="number" class= "Myinput" name="phone" placeholder ="Phone Number") 
            input(type="text" class= "Myinput" name="age" placeholder =" Your Age") 
            input(type="email" class= "Myinput" name="email" placeholder ="Your Email")  
            input(type="text" class= "Myinput" name="addresh" placeholder ="Addresh") 
            input(type="text" class= "Myinput" name="desk" placeholder ="enter Your Concern") 
            
            button(type="submit.pug" class='btn') 
                | Submit 
             
            

            
            
             
